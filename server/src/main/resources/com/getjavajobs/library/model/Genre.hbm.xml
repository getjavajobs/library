<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.getjavajobs.library.model.Genre" table="Genre">
        <id name="id" type="int">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="genreType" type="string">
            <column name="genre_type" />
        </property>
    </class>

</hibernate-mapping>